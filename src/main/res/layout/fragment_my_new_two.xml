<?xml version="1.0" encoding="utf-8"?>
<com.yjfshop123.live.ui.widget.shimmer.VerticalSwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/swipe_refresh"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_my"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <View
            android:id="@+id/view_status_bar"
            android:layout_width="match_parent"
            android:layout_height="22dp" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/more"
                android:layout_width="38dp"
                android:layout_height="37dp"
                android:layout_marginLeft="10dp"
                android:gravity="center"
                android:padding="10dp"
                android:src="@mipmap/my_more"
                android:textColor="@color/color_000000"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="@string/x_me"
                android:textColor="@color/color_000000"
                android:textSize="16dp"
                android:visibility="gone" />


        </FrameLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="8dp"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"


                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/username"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:includeFontPadding="false"
                        android:text="Hello"
                        android:textColor="@color/color_3B3026"
                        android:textSize="20sp" />

                    <RelativeLayout
                        android:id="@+id/upgrade"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="3dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingStart="4dp"
                        android:paddingEnd="8dp">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignStart="@+id/vip_level"
                            android:layout_alignTop="@+id/vip_level"
                            android:layout_alignEnd="@+id/vip_level"
                            android:layout_alignBottom="@+id/vip_level"
                            android:scaleType="fitXY"
                            android:src="@mipmap/level_one_huangguan_t" />

                        <TextView
                            android:id="@+id/vip_level"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingStart="32dp"
                            android:paddingTop="5dp"
                            android:paddingEnd="10dp"
                            android:paddingBottom="2dp"
                            android:text="lv0"
                            android:textAllCaps="true"
                            android:textColor="@color/white"
                            android:textSize="12sp" />


                    </RelativeLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/user_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:background="@drawable/bg_gradient_fae2ae_b28d51_4"
                    android:paddingStart="8dp"
                    android:paddingTop="1dp"
                    android:paddingEnd="8dp"
                    android:paddingBottom="1dp"
                    android:text="ID:"
                    android:textColor="@color/white"
                    android:textSize="12sp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/user_account"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"

                    android:text="@string/account"
                    android:textColor="@color/color_AFAFAF"
                    android:textSize="14sp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:paddingStart="8dp"
                    android:paddingEnd="8dp"
                    android:paddingTop="2dp"
                    android:paddingBottom="2dp"
                    android:gravity="center"
                    android:background="@drawable/bg_f9ebdf_6"
                    android:orientation="horizontal">


                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="16dp"
                        android:src="@mipmap/huo" />

                    <TextView
                        android:id="@+id/activity_num"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="4dp"
                        android:layout_weight="1"
                        android:text="@string/active_num_title"
                        android:textColor="@color/color_A98344"
                        android:textSize="14sp" />
                </LinearLayout>

            </LinearLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_alignParentEnd="true">

                <com.yjfshop123.live.server.widget.CircleImageView
                    android:id="@+id/autorImg"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:src="@drawable/splash_logo" />

                <LinearLayout
                    android:id="@+id/income1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/autorImg"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="-20dp"
                    android:background="@mipmap/yellow_rect"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingStart="12dp"
                    android:paddingTop="5dp"
                    android:paddingEnd="12dp"
                    android:paddingBottom="5dp"
                    android:visibility="gone">

                    <ImageView
                        android:layout_width="11dp"
                        android:layout_height="12dp"
                        android:src="@mipmap/qian_my" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="4dp"
                        android:gravity="center"
                        android:text="@string/my_income"
                        android:textColor="@color/white"
                        android:textSize="10dp" />
                </LinearLayout>
            </RelativeLayout>


        </RelativeLayout>

        <android.support.design.widget.CoordinatorLayout
            android:id="@+id/coordinatorLayout"
            android:layout_width="match_parent"

            android:layout_height="match_parent">

            <!--            <android.support.v4.widget.NestedScrollView-->
            <!--                android:layout_width="match_parent"-->
            <!--                android:layout_height="match_parent"-->
            <!--                android:fillViewport="true"-->
            <!--                app:layout_behavior="@string/appbar_scrolling_view_behavior">-->

            <android.support.v4.view.ViewPager
                android:id="@+id/viewPager"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:background="@color/color_FFFCF2"
                app:layout_behavior="@string/appbar_scrolling_view_behavior" />

            <!--            </android.support.v4.widget.NestedScrollView>-->

            <android.support.design.widget.AppBarLayout
                android:id="@+id/appbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_my"
                android:orientation="vertical"
                app:elevation="0dp"
                app:layout_behavior=".ui.widget.AppBarLayoutBehavior">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_scrollFlags="scroll|exitUntilCollapsed">


                    <LinearLayout
                        android:id="@+id/below"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="18dp"
                        android:layout_marginTop="30dp"
                        android:layout_marginEnd="18dp"
                        android:background="@mipmap/my_egg_bg"
                        android:minHeight="115dp"
                        android:orientation="horizontal"
                        android:paddingTop="24dp">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/my_gold_egg"
                                android:layout_width="42dp"
                                android:layout_height="47dp"
                                android:gravity="center"
                                android:src="@mipmap/gold_egg" />

                            <TextView
                                android:id="@+id/gold_egg_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp"
                                android:gravity="center"
                                android:text="0"
                                android:textColor="@color/color_B4B1AB"
                                android:textSize="14sp" />
                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical">


                            <ImageView
                                android:id="@+id/my_yin_egg"
                                android:layout_width="42dp"
                                android:layout_height="47dp"
                                android:gravity="center"
                                android:src="@mipmap/silver_egg" />

                            <TextView
                                android:id="@+id/yin_egg_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp"
                                android:gravity="center"
                                android:text="0"
                                android:textColor="@color/color_B4B1AB"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/my_bad_egg"
                                android:layout_width="42dp"
                                android:layout_height="47dp"

                                android:gravity="center"
                                android:src="@mipmap/bad_egg" />

                            <TextView
                                android:id="@+id/chou_num"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp"
                                android:gravity="center"
                                android:text="0"
                                android:textColor="@color/color_B4B1AB"
                                android:textSize="14sp" />

                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/target_reward_ll"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="18dp"
                        android:layout_marginTop="18dp"
                        android:layout_marginEnd="18dp"
                        android:visibility="gone">


                        <LinearLayout
                            android:id="@+id/twwww"
                            android:layout_width="0dp"
                            android:layout_height="71dp"
                            android:layout_weight="1"
                            android:background="@mipmap/wode_zhongjiang_one"
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:paddingStart="20dp">

                            <ImageView
                                android:layout_width="43dp"
                                android:layout_height="36dp"
                                android:scaleType="fitXY"
                                android:src="@mipmap/wode_jiang_huoyue" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"

                                android:orientation="vertical">


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:gravity="center"
                                    android:orientation="horizontal"

                                    >

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="@string/team_activity"
                                        android:textColor="@color/color_302011"
                                        android:textSize="12sp" />

                                    <TextView
                                        android:id="@+id/tartget_current_activity_num"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="10dp"
                                        android:layout_weight="1"
                                        android:text="0"
                                        android:textColor="@color/color_302011"
                                        android:textSize="16sp" />


                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginTop="6dp"
                                    android:gravity="center"
                                    android:orientation="horizontal"

                                    >

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="达到"
                                        android:textColor="@color/color_91775B"
                                        android:textSize="12sp" />

                                    <TextView
                                        android:id="@+id/tartget_need_activity_num"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="0"
                                        android:textColor="@color/color_297C61"
                                        android:textSize="12sp" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:text="，领取"
                                        android:textColor="@color/color_91775B"
                                        android:textSize="12sp" />

                                    <TextView
                                        android:id="@+id/reward_des"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:text="0"
                                        android:textColor="@color/color_822020"
                                        android:textSize="12sp" />
                                </LinearLayout>

                            </LinearLayout>


                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/recive_btn"
                            android:layout_width="wrap_content"
                            android:layout_height="71dp"
                            android:background="@mipmap/wode_zhongjiang_two"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:paddingStart="17dp"
                            android:paddingEnd="17dp">


                            <TextView
                                android:id="@+id/recive_btn_two"

                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:background="@drawable/recive_select"
                                android:enabled="false"

                                android:text="@string/receive_reward"
                                android:textColor="@color/color_763615"
                                android:textSize="16sp" />

                            <TextView
                                android:id="@+id/recive_progress"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:layout_marginTop="3dp"
                                android:background="@drawable/recive_select"
                                android:enabled="false"
                                android:text="已完成0%"
                                android:textColor="@color/color_91775B"
                                android:textSize="12sp" />
                        </LinearLayout>

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="32dp"
                        android:layout_marginTop="24dp"
                        android:layout_marginEnd="43dp"
                        android:layout_marginBottom="24dp"
                        android:orientation="horizontal">

                        <LinearLayout

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/buy_gold"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_horizontal"
                                android:orientation="vertical">

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <ImageView
                                        android:id="@+id/ooo"
                                        android:layout_width="38dp"
                                        android:layout_height="32dp"
                                        android:layout_centerHorizontal="true"
                                        android:scaleType="centerInside"
                                        android:src="@mipmap/buy_gold" />

                                    <TextView
                                        android:id="@+id/msg_count"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_alignEnd="@+id/ooo"
                                        android:background="@mipmap/red_yuan"
                                        android:enabled="false"
                                        android:gravity="center"
                                        android:minWidth="14dp"
                                        android:minHeight="14dp"
                                        android:textColor="@color/white"
                                        android:textSize="11sp"
                                        android:visibility="invisible" />
                                </RelativeLayout>


                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="12dp"
                                    android:text="@string/bug_gold_egg"
                                    android:textColor="@color/color_3B3026"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/invite_friend"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:gravity="center_horizontal"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="38dp"
                                    android:layout_height="32dp"
                                    android:scaleType="centerInside"
                                    android:src="@mipmap/invite_friend" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="12dp"

                                    android:text="@string/invite_friend"
                                    android:textColor="@color/color_3B3026"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />

                            </LinearLayout>
                        </LinearLayout>

                        <View
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_weight="1" />

                        <LinearLayout

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/my_team"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_horizontal"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="38dp"
                                    android:layout_height="32dp"
                                    android:scaleType="centerInside"
                                    android:src="@mipmap/my_team" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="12dp"

                                    android:text="@string/my_team"
                                    android:textColor="@color/color_3B3026"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/help_reward"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:gravity="center_horizontal"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="38dp"
                                    android:layout_height="32dp"
                                    android:scaleType="centerInside"
                                    android:src="@mipmap/zhutui" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="12dp"
                                    android:text="@string/help_reward"
                                    android:textColor="@color/color_3B3026"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                            </LinearLayout>
                        </LinearLayout>

                        <View
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_weight="1" />

                        <LinearLayout

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/income"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_horizontal"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="38dp"
                                    android:layout_height="32dp"
                                    android:scaleType="centerInside"
                                    android:src="@mipmap/shouzhimingxi" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="12dp"
                                    android:text="@string/shouzhifenxi"
                                    android:textColor="@color/color_3B3026"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/xuanppin_center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:gravity="center_horizontal"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="38dp"
                                    android:layout_height="32dp"
                                    android:scaleType="centerInside"
                                    android:src="@mipmap/xuanpin" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="12dp"
                                    android:text="@string/xuanpin_center"
                                    android:textColor="@color/color_3B3026"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_weight="1" />

                        <LinearLayout

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/task_center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_horizontal"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="38dp"
                                    android:layout_height="32dp"
                                    android:scaleType="centerInside"
                                    android:src="@mipmap/task_center" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="12dp"
                                    android:text="@string/task_center"
                                    android:textColor="@color/color_3B3026"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/trade_list"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:gravity="center_horizontal"
                                android:orientation="vertical"
                                android:visibility="gone">

                                <ImageView
                                    android:layout_width="38dp"
                                    android:layout_height="32dp"
                                    android:scaleType="centerInside"
                                    android:src="@mipmap/trade_list" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="12dp"
                                    android:text="@string/trade_list"
                                    android:textColor="@color/color_3B3026"
                                    android:textSize="14sp"
                                    android:textStyle="bold" />
                            </LinearLayout>

                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/choujiang_tips"
                        android:layout_width="match_parent"
                        android:layout_height="38dp"
                        android:layout_marginStart="17dp"
                        android:layout_marginEnd="18dp"
                        android:layout_marginBottom="16dp"
                        android:background="@mipmap/choujiangtixing"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingStart="16dp"
                        android:paddingEnd="16dp"
                        android:visibility="gone">

                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:src="@mipmap/gold_egg" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_weight="1"
                            android:text="砸金蛋,抽大奖"
                            android:textColor="@color/white"
                            android:textSize="12sp" />

                        <ImageView
                            android:layout_width="6dp"
                            android:layout_height="10dp"
                            android:src="@mipmap/right"
                            android:tint="@color/white" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/tip_update"
                        android:layout_width="match_parent"
                        android:layout_height="38dp"
                        android:layout_marginStart="17dp"
                        android:layout_marginEnd="18dp"
                        android:layout_marginBottom="16dp"
                        android:background="@mipmap/choujiangtixing"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingStart="16dp"
                        android:paddingEnd="16dp"
                        android:visibility="gone">

                        <ImageView
