<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="6dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="6dp"
        android:layout_marginBottom="10dp"
        android:orientation="horizontal">

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="160dp"
            android:layout_gravity="bottom"
            android:layout_marginLeft="6dp"
            android:layout_marginRight="6dp"
            android:layout_weight="1.0">

            <View
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="40dp"
                android:background="@drawable/bg_gradient_fdf7f1_fee8d5_9" />

            <com.yjfshop123.live.server.widget.CircleImageView
                android:id="@+id/list_top_icon_2"
                android:layout_width="58dp"
                android:layout_height="58dp"
                android:visibility="gone"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="10dp"
                android:scaleType="centerCrop"
                android:src="@color/white" />

            <ImageView
                android:layout_width="60dp"
                android:layout_height="79dp"
                android:layout_gravity="center_horizontal"
                android:src="@mipmap/yinbei" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal|bottom"
                android:layout_marginBottom="8dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/list_top_name_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:maxLength="7"
                    android:text="@string/xwd"
                    android:textColor="@color/color_333333"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/list_top_sex_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/_mm_details_01"
                    android:drawableLeft="@drawable/male_icon"
                    android:gravity="center"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:text=" 0"
                    android:textColor="@color/white"
                    android:textSize="9dp" />

                <TextView
                    android:id="@+id/list_top_gold_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="8dp"
                    android:text="@string/jb_hint_4"
                    android:textColor="@color/color_999999"
                    android:textSize="12dp" />

            </LinearLayout>

        </FrameLayout>


        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="180dp"
            android:layout_gravity="bottom"
            android:layout_marginLeft="6dp"
            android:layout_marginRight="6dp"
            android:layout_weight="1.0">

            <View
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="46dp"
                android:background="@drawable/bg_gradient_fdf7f1_fee8d5_9" />

            <com.yjfshop123.live.server.widget.CircleImageView
                android:id="@+id/list_top_icon_1"
                android:layout_width="68dp"
                android:layout_height="68dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="13dp"
                android:scaleType="centerCrop"
                android:visibility="gone"

                android:src="@color/white" />

            <ImageView
                android:layout_width="70dp"
                android:layout_height="93dp"
                android:layout_gravity="center_horizontal"
                android:src="@mipmap/jinbei" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal|bottom"
                android:layout_marginBottom="8dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/list_top_name_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:maxLength="7"
                    android:text="@string/xwd"
                    android:textColor="@color/color_333333"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/list_top_sex_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/_mm_details_01"
                    android:drawableLeft="@drawable/male_icon"
                    android:gravity="center"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:text=" 0"
                    android:textColor="@color/white"
                    android:textSize="9dp" />

                <TextView
                    android:id="@+id/list_top_gold_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="8dp"
                    android:text="@string/jb_hint_4"
                    android:textColor="@color/color_999999"
                    android:textSize="12dp" />

            </LinearLayout>

        </FrameLayout>


        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="160dp"
            android:layout_gravity="bottom"
            android:layout_marginLeft="6dp"
            android:layout_marginRight="6dp"
            android:layout_weight="1.0">

            <View
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="40dp"
                android:background="@drawable/bg_gradient_fdf7f1_fee8d5_9" />

            <com.yjfshop123.live.server.widget.CircleImageView
                android:id="@+id/list_top_icon_3"
                android:layout_width="58dp"
                android:layout_height="58dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="10dp"
                android:visibility="gone"
                android:scaleType="centerCrop"
                android:src="@color/white" />

            <ImageView
                android:layout_width="60dp"
                android:layout_height="79dp"
                android:layout_gravity="center_horizontal"
                android:src="@mipmap/tongbei" />


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal|bottom"
                android:layout_marginBottom="8dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/list_top_name_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:maxLength="7"
                    android:text="@string/xwd"
                    android:textColor="@color/color_333333"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/list_top_sex_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/_mm_details_01"
                    android:drawableLeft="@drawable/male_icon"
                    android:gravity="center"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:text=" 0"
                    android:textColor="@color/white"
                    android:textSize="9dp" />

                <TextView
                    android:id="@+id/list_top_gold_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="8dp"
                    android:text="@string/jb_hint_4"
                    android:textColor="@color/color_999999"
                    android:textSize="12dp" />

            </LinearLayout>
        </FrameLayout>

    </LinearLayout>


</LinearLayout>